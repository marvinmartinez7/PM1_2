<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PM1_2.Views.PageLista">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding name}"
               
                VerticalOptions="CenterAndExpand" 
               
                HorizontalOptions="CenterAndExpand"  x:Name="lbresultado1" TextColor="White" FontSize="16" />
            <Label Text="{Binding apellidos}"
               
                VerticalOptions="CenterAndExpand" 
               
                HorizontalOptions="CenterAndExpand"  x:Name="lbresultado" TextColor="White" FontSize="16" />
            <Label Text="{Binding edad}"
               
                VerticalOptions="CenterAndExpand" 
               
                HorizontalOptions="CenterAndExpand"  x:Name="lbresultado3" TextColor="White" FontSize="16" />
            <Label Text="{Binding correo}"
               
                VerticalOptions="CenterAndExpand" 
               
                HorizontalOptions="CenterAndExpand"  x:Name="lbresultado2" TextColor="White" FontSize="16" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>